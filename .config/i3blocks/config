[wifi]
command=/usr/libexec/i3blocks/wifi
INTERFACE=wlo1
markup=pango
interval=10

[ssid]
command=/usr/libexec/i3blocks/ssid
INTERFACE=wlo1
markup=pango
interval=10

[volume]
command=/usr/libexec/i3blocks/volume-pulseaudio
markup=pango
interval=1

[memory]
command=/usr/libexec/i3blocks/memory
markup=pango
interval=10

[cpu_usage2]
command=/usr/libexec/i3blocks/cpu_usage2
markup=pango
interval=persist

[xkb_layout]
command=/usr/libexec/i3blocks/xkb_layout
interval=persist
markup=pango

[battery]
command=/usr/libexec/i3blocks/battery2
markup=pango
interval=10

[time]
command=date +"%a %d %b %H:%M"
interval=5
